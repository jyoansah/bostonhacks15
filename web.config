<configuration>
    <system.web>
        <compilation
            debug="true">
        </compilation>
    </system.web>
</configuration>
<rewrite>
    <rules>
        <rule name="RewriteAPI" stopProcessing="true">
            <match url="^api/(.+)$" ignoreCase="false" />
            <action type="Rewrite" url="/" appendQueryString="false" />
        </rule>
    </rules>
</rewrite>